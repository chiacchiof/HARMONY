{
  "description": "Sistema di alimentazione elettrica con ridondanza",
  "topEvent": "gate-1",
  "elements": [
    {
      "type": "event",
      "id": "event-1",
      "name": "Guasto Alimentatore Principale",
      "description": "Guasto completo dell'alimentatore principale",
      "failureRate": 0.001,
      "failureProbabilityDistribution": { "type": "exponential", "lambda": 0.001 }
    },
    {
      "type": "event",
      "id": "event-2",
      "name": "Guasto Alimentatore Backup",
      "description": "Guasto completo dell'alimentatore di backup",
      "failureRate": 0.001,
      "failureProbabilityDistribution": { "type": "exponential", "lambda": 0.001 }
    },
    {
      "type": "event",
      "id": "event-3",
      "name": "Guasto UPS",
      "description": "Guasto del sistema UPS",
      "failureRate": 0.0005,
      "failureProbabilityDistribution": { "type": "exponential", "lambda": 0.0005 }
    },
    {
      "type": "gate",
      "id": "gate-1",
      "name": "Perdita Alimentazione Sistema",
      "gateType": "OR",
      "description": "Top event: perdita completa dell'alimentazione elettrica"
    },
    {
      "type": "gate",
      "id": "gate-2",
      "name": "Guasto Alimentatori",
      "gateType": "AND",
      "description": "Entrambi gli alimentatori sono guasti"
    }
  ],
  "connections": [
    {
      "source": "event-1",
      "target": "gate-2"
    },
    {
      "source": "event-2",
      "target": "gate-2"
    },
    {
      "source": "gate-2",
      "target": "gate-1"
    },
    {
      "source": "event-3",
      "target": "gate-1"
    }
  ]
}
